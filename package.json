{
  "name": "baseui-tanstack-form",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/baseui-tanstack-form.cjs.js",
  "module": "./dist/baseui-tanstack-form.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/baseui-tanstack-form.es.js",
      "require": "./dist/baseui-tanstack-form.cjs.js"
    }
  },
  "author": "KahWee Teng <tengkahwee@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "clean": "rm -rf dist coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "storybook": "NODE_OPTIONS=--no-warnings storybook dev -p 6006",
    "build:storybook": "NODE_OPTIONS=--no-warnings storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0-alpha.1",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-interactions": "^8.6.11",
    "@storybook/addon-links": "^8.6.11",
    "@storybook/builder-vite": "^8.6.11",
    "@storybook/react": "^8.6.10",
    "@storybook/react-vite": "^8.6.10",
    "@tanstack/react-form": "^1.3.1",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.2.1",
    "baseui": "^15.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.6.10",
    "styletron-engine-atomic": "^1.6.2",
    "ts-jest": "^29.3.0",
    "typescript": "^5.3.3",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^3.7.2",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@tanstack/react-form": "^1.3.1",
    "baseui": ">= 15.0.0",
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0",
    "zod": "^3.22.4"
  },
  "keywords": [
    "react",
    "reactjs",
    "tanstack",
    "tanstack-form",
    "forms",
    "baseui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kahwee/baseui-tanstack-form.git"
  },
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2,
    "printWidth": 80
  }
}
