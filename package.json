{
  "name": "baseui-tanstack-form",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/baseui-tanstack-form.cjs.js",
  "module": "./dist/baseui-tanstack-form.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/baseui-tanstack-form.es.js",
      "require": "./dist/baseui-tanstack-form.cjs.js"
    }
  },
  "author": "KahWee Teng <tengkahwee@gmail.com>",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build && tsc --emitDeclarationOnly",
    "clean": "rm -rf dist coverage",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "typecheck": "npx tsc --noEmit --skipLibCheck",
    "storybook": "npx storybook dev -p 6006",
    "build:storybook": "npx storybook build",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest run --coverage",
    "test:ui": "npx vitest --ui",
    "prepare": "husky install",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-links": "^10.1.10",
    "@storybook/builder-vite": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@tanstack/react-form": "^1.27.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "npm:types-react@^19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@^19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "baseui": "^16.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.10",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "prettier": "^3.6.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.1.10",
    "styletron-engine-atomic": "^1.6.2",
    "styletron-react": "^6.1.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "zod": "^4.1.12"
  },
  "overrides": {
    "react-input-mask": "npm:@mona-health/react-input-mask@^3.0.3"
  },
  "peerDependencies": {
    "@tanstack/react-form": "*",
    "baseui": ">= 15.0.0",
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0",
    "zod": "*"
  },
  "keywords": [
    "react",
    "reactjs",
    "tanstack",
    "tanstack-form",
    "forms",
    "baseui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kahwee/baseui-tanstack-form.git"
  },
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2,
    "printWidth": 80
  }
}
